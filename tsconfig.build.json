{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "test",
    "dist",
    "api",
    "scripts",
    "prisma",
    "*.ts",
    "**/*spec.ts"
  ]
}